<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:viewGroupName="LinearRoot">

            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout
                android:id="@+id/myLinearLayout1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="5dp"
                app:viewGroupName="MyLinearLayout1">

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView1_1"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_green_light"
                    app:viewName="MyView1-1" />

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView1_2"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_orange_light"
                    app:viewName="MyView1-2" />

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView1_3"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_green_light"
                    app:viewName="MyView1-3" />
            </com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout>

            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout
                android:id="@+id/myLinearLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="5dp"
                app:viewGroupName="MyLinearLayout2">

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView2_1"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_orange_light"
                    app:viewName="MyView2-1" />

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView2_2"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_green_light"
                    app:viewName="MyView2-2" />

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView2_3"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_orange_light"
                    app:viewName="MyView2-3" />

            </com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout>


            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout
                android:id="@+id/myLinearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="5dp"
                app:viewGroupName="MyLinearLayout3">

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView3_1"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_orange_light"
                    app:viewName="MyView3-1" />

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView3_2"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_green_light"
                    app:viewName="MyView3-2" />

                <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                    android:id="@+id/myView3_3"
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_orange_light"
                    app:viewName="MyView3-3" />

            </com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout>

        </com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyLinearLayout>
        <FrameLayout
            android:layout_width="400dp"
            android:layout_height="400dp">
            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                android:layout_width="250dp"
                android:layout_height="250dp"
                android:layout_gravity="top|start"
                app:viewName="my-View1"
                android:background="@android:color/holo_orange_light"/>
            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                android:layout_width="250dp"
                android:layout_height="250dp"
                android:layout_gravity="top|end"
                app:viewName="my-View2"
                android:background="@android:color/holo_red_light"/>
            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                android:layout_width="250dp"
                android:layout_height="250dp"
                android:layout_gravity="bottom|start"
                app:viewName="my-View3"
                android:background="@android:color/holo_red_light"/>
            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                android:layout_width="250dp"
                android:layout_height="250dp"
                android:layout_gravity="bottom|end"
                app:viewName="my-View4"
                android:background="@android:color/holo_orange_light"/>
            <com.schrodinger.androidcustomview.customViewGroup.testInterceptTouchEvent.MyView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                app:viewName="my-View5"
                android:background="@android:color/holo_blue_light"/>
        </FrameLayout>
    </LinearLayout>

</layout>
